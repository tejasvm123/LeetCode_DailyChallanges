class Solution {
public:
    int countOdds(int low, int high) {
        if(low % 2 == 0 && high % 2 == 0){
            return (high - low )/2;
        }
        return(high - low)/2 + 1;
    }
};

class Solution {
public:
    int countOdds(int low, int high) {
        int counter = 0;

        for(int i=low; i<(high + 1); i++){
            if(i % 2 != 0){
                counter++;
            }
        }
        return counter;
    }
};
